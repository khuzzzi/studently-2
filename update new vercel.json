{
  "version": 2,
  "builds": [
    {
      "src": "studently-backend/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "studently-frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "studently-backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "studently-frontend/$1"
    }
  ]
}
